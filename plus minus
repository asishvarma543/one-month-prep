def plusMinus(arr):
    # Write your code here
    length=len(arr)
    pcount=0
    ncount=0
    zcount=0
    for x in arr:
        if(x>0):
            pcount+=1
        elif(x<0):
            ncount+=1
        else:
            zcount+=1
    print(pcount/length)
    print(ncount/length)
    print(zcount/length)
